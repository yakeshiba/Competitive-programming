    "#316 FizzBuzz ３つの定数のうち，どれかの倍数の個数　最小公倍数\n",
    "import fractions\n",
    "def lcm(a,b):\n",
    "    l = a * b // fractions.gcd(a, b)\n",
    "    return l\n",
    "N = int(input())\n",
    "a,b,c = map(int,input().split())\n",
    "print(N//a+N//b+N//c-N//lcm(a,b)-N//lcm(a,c)-N//lcm(b,c)+N//lcm(lcm(a,b),c))"
   ]
