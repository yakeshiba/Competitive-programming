    "#B\n",
    "n, m, k = map(int,input().split())\n",
    "\n",
    "for i in range(min(n,m)+1):\n",
    "    for j in range(max(n,m)+1):\n",
    "        tmp = max(n,m)*i + min(n,m)*j - 2*i*j\n",
    "        if tmp == k:\n",
    "            print('Yes')\n",
    "            exit()\n",
    "                \n",
    "print('No')"
   ]
  }
