   "source": [
    "#A\n",
    "import itertools\n",
    "\n",
    "n = int(input())\n",
    "A = [0]+list(map(int,input().split()))\n",
    "\n",
    "cumsum = list(itertools.accumulate(A))\n",
    "\n",
    "d = {}\n",
    "\n",
    "ans = 0\n",
    "for i in cumsum:\n",
    "    if i not in d:\n",
    "        d[i] = 1\n",
    "    else:\n",
    "        ans += d[i]\n",
    "        d[i] += 1\n",
    "\n",
    "print(ans)"
   ]
  }
