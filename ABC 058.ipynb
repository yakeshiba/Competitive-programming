   "source": [
    "#C\n",
    "n = int(input())\n",
    "\n",
    "alp = [float(\"inf\")]*200\n",
    "\n",
    "for i in range(n):\n",
    "    S = input()\n",
    "    tmp = [0]*200\n",
    "    for s in S:\n",
    "        tmp[ord(s)] += 1\n",
    "    for ind in range(200):\n",
    "        alp[ind] = min(alp[ind],tmp[ind])\n",
    "\n",
    "ans = \"\"\n",
    "for i in range(200):\n",
    "    ans += chr(i)*alp[i]\n",
    "    \n",
    "print(ans)"
   ]
  }
