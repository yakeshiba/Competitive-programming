   "source": [
    "#C\n",
    "n, k = map(int,input().split())\n",
    "\n",
    "ans = 0\n",
    "#a,b,c = pk,qk,rk\n",
    "pk = n//k\n",
    "ans += pk**3\n",
    "\n",
    "#a,b,c = pk/2,qk/2,rk/2\n",
    "if k%2 == 0:\n",
    "    qk = n//(k//2) - pk\n",
    "    ans += qk**3\n",
    "\n",
    "print(ans)"
   ]
  }
