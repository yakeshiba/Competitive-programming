   "source": [
    "#C\n",
    "from collections import Counter\n",
    "\n",
    "n, k = map(int,input().split())\n",
    "A = list(map(int,input().split()))\n",
    "\n",
    "cnt = Counter(A)\n",
    "\n",
    "s = len(cnt.keys())\n",
    "\n",
    "ans = 0\n",
    "kosu = sorted(cnt.values())\n",
    "ind = 0\n",
    "while s > k:\n",
    "    s -= 1\n",
    "    ans += kosu[ind]\n",
    "    ind += 1\n",
    "\n",
    "print(ans)"
   ]
  }
 ],
