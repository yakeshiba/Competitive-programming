    "N,K = map(int,input().split())\n",
    "num = [None]*N\n",
    "for i in range(N):\n",
    "    a,b = map(int,input().split())\n",
    "    num[i] = (a,b)\n",
    "num.sort()\n",
    "\n",
    "cnt = 0\n",
    "for v,n in num:\n",
    "    cnt += n\n",
    "    if cnt >= K:\n",
    "        print(v)\n",
    "        break"
   ]
  }
