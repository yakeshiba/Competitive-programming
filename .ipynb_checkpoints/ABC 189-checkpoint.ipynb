   "source": [
    "#C\n",
    "n = int(input())\n",
    "A = list(map(int,input().split()))\n",
    "\n",
    "ans = min(A)*n\n",
    "for i, now in enumerate(A):\n",
    "    minim = now\n",
    "    for j in range(i,n):\n",
    "        minim = min(minim,A[j])\n",
    "        ans = max(ans, minim*(j-i+1))\n",
    "    \n",
    "print(ans)"
   ]
  }
