   "source": [
    "#A\n",
    "import numpy as np\n",
    "\n",
    "N = int(input())\n",
    "A = np.array([0] + list(map(int,input().split())))\n",
    "\n",
    "cumsum = np.cumsum(A)\n",
    "\n",
    "for i in range(1,N+1):\n",
    "    V = cumsum[i:] - cumsum[:-i]#[:-i]とすると最後からi個を除いた部分を切り取れる\n",
    "    print(np.max(V))"
   ]
  }
