   "source": [
    "#DPL_1_D: Longest Increasing Subsequence\n",
    "import bisect\n",
    "n = int(input())\n",
    "LIS = [-1]\n",
    "\n",
    "for _ in range(n):\n",
    "    a = int(input())\n",
    "    if LIS[-1] < a:\n",
    "        LIS.append(a)\n",
    "    else:\n",
    "        ind = bisect.bisect_left(LIS, a)\n",
    "        LIS[ind] = a\n",
    "        \n",
    "print(len(LIS)-1)"
   ]
  }
