   "source": [
    "#B\n",
    "r, g, b, n = map(int,input().split())\n",
    "\n",
    "ans = 0\n",
    "for i in range(0,n+1):\n",
    "    for j in range(0,n-i+1):\n",
    "        if r*i+g*j > n:\n",
    "            break\n",
    "        if (n-r*i-g*j)%b == 0:\n",
    "            ans += 1\n",
    "print(ans)"
   ]
  }
