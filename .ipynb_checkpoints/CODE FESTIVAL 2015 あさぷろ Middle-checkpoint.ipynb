   "source": [
    "#A\n",
    "n, k, m, r = map(int,input().split())\n",
    "\n",
    "S = []\n",
    "for _ in range(n-1):\n",
    "    S.append(int(input()))\n",
    "\n",
    "S.sort(reverse=True)\n",
    "\n",
    "if sum(S[:k])-k*r >= 0:\n",
    "    print(0)\n",
    "else:\n",
    "    an = k*r - sum(S[:k-1])\n",
    "    ans = -(-an//1)\n",
    "    if ans <= m:\n",
    "        print(ans)\n",
    "    else:\n",
    "        print(-1)"
   ]
  }
