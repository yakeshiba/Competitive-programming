    "#C\n",
    "n = int(input())\n",
    "\n",
    "dp = [0]*n\n",
    "\n",
    "if n>=3:\n",
    "    dp[2] = 1\n",
    "\n",
    "for i in range(3,n):\n",
    "    dp[i] = (dp[i-3]+dp[i-2]+dp[i-1])%10007\n",
    "    \n",
    "print(dp[n-1])"
   ]
