    "s, c = map(int,input().split())\n",
    "\n",
    "ans = min(s,c//2)\n",
    "s -= ans\n",
    "c -= 2*ans\n",
    "\n",
    "if not s:\n",
    "    ans += c//4\n",
    "\n",
    "print(ans)"
