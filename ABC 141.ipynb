   "source": [
    "import heapq\n",
    "\n",
    "N, M = map(int,input().split())\n",
    "A = list(map(lambda x: -int(x),input().split()))\n",
    "\n",
    "heapq.heapify(A)\n",
    "\n",
    "for i in range(M):\n",
    "    tmp = -heapq.heappop(A)\n",
    "    heapq.heappush(A,-(tmp//2))\n",
    "print(-sum(A))"
   ]
